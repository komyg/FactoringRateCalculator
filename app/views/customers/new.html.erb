<!-- app/views/users/new.html.erb -->

<h1>Signup!</h1>

<%= form_for :customer, url: '/users' do |f| %>

  Name: <%= f.text_field :name %>
  Email: <%= f.text_field :email %>
  Password: <%= f.password_field :password %>
  Company: <%= f.text_field :company_name %>
  <%= f.submit "Submit" %>

<% end %>
